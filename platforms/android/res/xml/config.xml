<?xml version='1.0' encoding='utf-8'?>
<widget id="com.career.wrap.tamil.payanam" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SpinnerDialog">
        <param name="android-package" value="hu.dpal.phonegap.plugins.SpinnerDialog" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <icon density="mdpi" src="res/android/payanam_mdpi.png" />
    <icon density="hdpi" src="res/android/payanam_hdpi.png" />
    <icon density="xhdpi" src="res/android/payanam_xhdpi.png" />
    <icon density="xxhdpi" src="res/android/payanam_xxhdpi.png" />
    <icon density="xxxhdpi" src="res/android/payanam_xxxhdpi.png" />
    <name>Tamil Payanam</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
